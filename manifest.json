

{
    "manifest_version": 3,
    "name": "Shulker Modpack Updater",
    "version": "1.0",
    "description": "Extension that works with the Shulker Modpack Manager to add mods to a modpack",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      },
      "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://www.curseforge.com/minecraft/mc-mods/*"
    
          ]
        }
      ]
  }